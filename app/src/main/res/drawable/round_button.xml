        <shape android:shape="oval">
            <solid android:color="#ffff"/>
            <size android:width="48dp"
                android:height="48dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#ffff"/>
            <size android:width="48dp"
                android:height="48dp"/>
        </shape>
    </item>
</selector>